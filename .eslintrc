{
	"env": {
		"browser": true,
		"es6": true,
		"jasmine": true,
		"node": true
	},
	"plugins": [
		"jasmine",
		"react",
		"vue"
	],
	"parserOptions": {
		"parser": "babel-eslint",
		"sourceType": "module",
		"allowImportExportEverywhere": false,
		"codeFrame": false
	},
	"globals": {
		"inject": true,
		"react": true,
		"vue": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:vue/recommended"
	],
	"rules": {
		"vue/html-indent": 0
	}
}
