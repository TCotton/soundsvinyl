{
	"name": "soundsvinyl.co",
	"version": "1.34.1",
	"description": "Music recommendation site",
	"main": "server.js",
	"scripts": {
		"test:back": "ava -v node/test/test.js",
		"test": "cross-env NODE_ENV=test karma start --require source-map-support/register test/karma.conf.js",
		"test:unit": "cross-env NODE_ENV=testing jest --testMatch='**/?(*.)+(test).js?(x)' --coverage",
		"test:node": "cross-env NODE_ENV=test jest --testPathPattern='./tests/.*\\.spec\\.js$' --env=node --detectOpenHandles --runInBand --forceExit",
		"build": "cross-env NODE_ENV=production webpack --mode production ./app/single-spa-config.js --output ./dist/bundle.js",
		"start:dev": "cross-env NODE_ENV=development webpack-dev-server --https --cert ./nodeService/config/server.crt --key ./nodeService/config/server.key --mode development ./app/single-spa-config.js --open",
		"start": "node --experimental-modules ./server.js",
		"start:nodeDev": "nodemon --inspect --experimental-modules  ./server.js",
		"start:static": "nodemon -r source-map-support/register --experimental-modules ./node/bin/www",
		"lint": "eslint 'app/**' 'nodeService/**' --ignore-pattern '*.spec.js'",
		"lint:fix": "eslint --fix 'app/**' 'nodeService/**' --ignore-pattern '*.spec.js'",
		"sass-lint": "sass-lint  --config .sasslintrc '**/*.scss' --verbose --no-exit --ignore './node_modules/**/**/**/**/*.scss'",
		"ssr": "cross-env NODE_ENV=production webpack --mode production node ./node/server.js",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"clearJest": "jest --clearCache && npm run test -- -u",
		"format": "prettier-standard --format {app,nodeService}/**/**/**/**/**/*.{js,jsx}"
	},
