{
  "name": "soundsvinyl.co",
  "version": "1.0.0",
  "description": "Music recommendation site",
  "main": "index.js",
  "scripts": {
    "test:front": "cross-env NODE_ENV=test karma start --require source-map-support/register test/karma.conf.js",
    "test:back": "ava -v node/test/test.js",
    "test:node": "NODE_ENV=test node --max-old-space-size=16384 --optimize-for-size node_modules/nyc/bin/nyc.js node_modules/mocha/bin/_mocha --timeout 15000 --require test-entry.js \"nodeService/__test__/**/*.js\"",
    "build": "cross-env NODE_ENV=production webpack --mode production ./app/front/index.js --output ./dist/bundle.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --https --cert ./nodeService/config/server.crt --key ./nodeService/config/server.key --mode development ./app/single-spa-config.js --open",
    "start": "node --experimental-modules ./server.js",
    "start:nodeDev": "nodemon --experimental-modules  ./server.js",
    "start:static": "nodemon -r source-map-support/register --experimental-modules ./node/bin/www",
    "lint": "eslint app/**",
    "sass-lint": "sass-lint  --config .sasslintrc '**/*.scss' --verbose --no-exit --ignore './node_modules/**/**/**/**/*.scss'",
    "ssr": "cross-env NODE_ENV=production webpack --mode production node ./node/server.js",
    "singel": "singel ./app/front/**.js --ignore ./app/front/spec/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCotton/soundsvinyl.co.git"
  },
  "keywords": [
    "react",
    "vue"
  ],
  "author": "Andy Walpole",
  "license": "CC-BY-NC-4.0",
  "bugs": {
