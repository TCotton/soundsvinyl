  "description": "Music recommendation site",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "webpack-dev-server",
    "test": "",
    "eject": "",
    "dev": "webpack --mode development ./src/front/index.js --output  ./src/front/main.js",
    "build": "webpack --mode production",
    "lint": "npm run lint:sass  && npm run lint:js",
    "lint:sass": "./node_modules/.bin/sass-lint -c .sass-lint.yml -v -q -s scss \"src/**/*.scss\"",
    "lint:js": "./node_modules/.bin/eslint \"src/front-non-library/**/**/*.js\" \"test/karma.conf.js\"",
    "node": "node --experimental-modules ./node/index.mjs",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --inline --watch --progress --open",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --color --open",
    "cleanUp": "npm rebuild && npm cache verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCotton/soundsvinyl.co.git"
