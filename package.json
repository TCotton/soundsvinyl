{
  "name": "soundsvinyl.co",
  "version": "1.0.0",
  "description": "Music recommendation site",
  "main": "index.js",
  "scripts": {
    "test:front": "cross-env NODE_ENV=test nyc --all karma start --require source-map-support/register test/karma.conf.js",
    "test:back": "ava -v node/test/test.js",
    "build": "cross-env NODE_ENV=production webpack --mode production ./app/front/index.js --output ./dist/bundle.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --mode development ./app/single-spa-config.js --open",
    "start": "nodemon -r source-map-support/register ./node/bin/www",
    "lint": "eslint app/**",
    "ssr": "cross-env NODE_ENV=production webpack --mode production node ./node/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCotton/soundsvinyl.co.git"
  },
  "keywords": [
    "react"
  ],
